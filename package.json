    "@vitejs/plugin-vue": "^1.9.2",
    "date-fns": "^2.24.0",
    "element-plus": "^1.1.0-beta.19",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "v-network-graph": "^0.2.3",
    "vitepress": "^0.19.2",
    "vue-tsc": "^0.3.0"
